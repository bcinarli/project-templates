{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "__DEV__": false,
    "__PROD__": false
  },
  "rules": {
    "no-console": "off",
    "no-undef": "off",
    "no-unused-vars": "off"
  }
}

